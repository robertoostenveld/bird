		"titles": {
			"title": "EEG responses to facial attractiveness, with ERP cluster analysis and MVPA classification"
		},
		"publisher": "DataverseNL",
		"publicationYear": "2023",
		"resourceType": {
			"_resourceTypeGeneral": "Dataset"
		},
		"descriptions": {
			"description": {
				"_descriptionType": "Abstract",
				"__text": "EEG data was recorded while 80 participants viewed 240 images of faces with varying levels of attractiveness. Participants indicated a subjective attractiveness rating following each image. These ratings were used to create subsets of the 10% highest, 10% lowest, and 10% middlemost rated images on the participant level. We performed cluster based ERP analyses (script included) and MVPA classification based on the whole scalp 3 second EEG patterns (script included)."
			}
		},
